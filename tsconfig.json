{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "types": [
      "webpack-env"
    ],
    "esModuleInterop": true,
    "allowSyntheticDefaultImports": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "resolveJsonModule": true
  }
}
